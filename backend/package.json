    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0"
