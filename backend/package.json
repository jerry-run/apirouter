{
  "name": "apirouter-backend",
  "version": "0.1.0",
  "description": "APIRouter backend service",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
